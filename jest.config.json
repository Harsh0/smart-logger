{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "coverageThreshold": {
        "global": {
            "statements": 90,
            "branches": 50,
            "functions": 100,
            "lines": 80
        }
    }
}
